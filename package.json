    "@tiptap/starter-kit": "2.5.7",
    "apexcharts": "3.51.0",
    "axios": "1.7.2",
    "casbin": "5.30.0",
    "dayjs": "1.11.12",
    "dotenv": "16.4.5",
    "js-cookie": "3.0.5",

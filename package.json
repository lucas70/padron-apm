{
  "name": "nextjs-base-frontend",
  "version": "2.0.0",
  "description": "Frontend base con Next.js, MUI v5 y TypeScript",
  "author": "agetic@agetic.gob.bo",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "lint": "next lint",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "preinstall": "mkdir -p node_modules; touch ./node_modules/.metadata_never_index",
    "release": "commit-and-tag-version",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:e2e": "playwright test",
    "test:codegen": "npx playwright codegen -b chromium",
    "test:codegen:mobile": "npx playwright codegen -b chromium --device='iPhone 13'",
    "format-check": "prettier --check 'pages/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'pages/**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
